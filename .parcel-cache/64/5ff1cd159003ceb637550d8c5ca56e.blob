* {
  font-family: "Poppins", sans-serif;
  margin: 0;
  padding: 0;
}
.container {
  width: 1140px;
  margin: 0 auto;
}
.promo {
  min-height: 100vh;
  position: relative;
  background: url("mebg.HASH_REF_739dc8bcbc2c38b3594dd24df1e1e866.jpg") no-repeat 50%/cover;
  padding-top: 205px;
}
.promo__title {
  width: 530px;
  margin-top: 10px;
}
.promo__btns {
  margin-top: 25px;
  display: flex;
  width: 250px;
  align-items: center;
  justify-content: space-between;
}
.promo__link {
  color: #212529;
  font-weight: bold;
  text-decoration: none;
}
.promo__link:hover {
  text-decoration: none;
  color: white;
}
.promo__mixin {
  position: relative;
  z-index: 2;
  padding-left: 10px;
}
.promo__mixin:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -25%;
  left: 0;
  width: 36px;
  height: 36px;
  border-radius: 100%;
  background: #FFA501;
}
.title {
  font-weight: 700;
  font-size: 36px;
  line-height: 60px;
  margin: 0;
}
.section__subtitle {
  font-weight: 700;
  font-size: 36px;
  line-height: 60px;
  margin: 0;
  text-align: center;
}
.section__title {
  text-align: center;
  position: relative;
}
.section__title:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -25%;
  left: 0;
  width: 36px;
  height: 36px;
  border-radius: 100%;
  background: #FFA501;
}
.section__title:before {
  top: -8px;
  left: 50%;
  transform: translateX(-50%);
}
.fz16 {
  font-size: 16px;
  line-height: 24px;
  margin: 0;
}
.fz20 {
  font-size: 20px;
  line-height: 30px;
  margin: 0;
}
.fz48 {
  font-size: 48px;
  margin: 0;
}
.fz14 {
  font-size: 14px;
  line-height: 21px;
  margin: 0;
}
.fz36 {
  font-size: 36px;
  line-height: 60px;
  margin: 0;
}
.fz12 {
  font-size: 12px;
  line-height: 18px;
  margin: 0;
}
.btn {
  height: 45px;
  background: #FFA501;
  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.15);
  padding: 12px 25px;
}
.sidepanel {
  position: fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  height: 340px;
  z-index: 3;
  top: 230px;
  left: 50px;
}
.sidepanel__divider {
  width: 2px;
  height: 70px;
  background-color: #212529;
  margin-left: 2px;
}
.sidepanel__text {
  height: 180px;
  width: 25px;
}
.sidepanel__text span {
  transform: rotate(-90deg) translate(-20%, -260%);
  display: block;
  width: 150px;
}
.sidepanel__link {
  margin-bottom: 15px;
}
.sidepanel__link:hover svg path {
  fill: blueviolet;
}
.hamburger {
  position: absolute;
  top: 20px;
  right: 30px;
  height: 21px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-end;
  cursor: pointer;
}
.hamburger span {
  display: block;
  width: 30px;
  height: 3px;
  background-color: #000;
}
.hamburger span.long {
  width: 36px;
}
.menu {
  position: fixed;
  top: 0;
  left: -100%;
  width: 100vw;
  height: 100vh;
  z-index: 10;
  visibility: hidden;
  opacity: 0;
  transition: all 0.6s;
}
.menu.active {
  left: 0;
  visibility: visible;
  opacity: 1;
}
.menu__block {
  position: relative;
  width: 345px;
  background-color: #030303;
  height: 100%;
  padding-top: 140px;
  transition: all 0.6s;
  z-index: 10;
}
.menu__close {
  position: absolute;
  top: 25px;
  right: 25px;
  cursor: pointer;
  transition: all 0.7s;
}
.menu__close:hover {
  transform: rotate(90deg);
}
.menu__close:hover path {
  fill: #ffffff;
}
.menu__list {
  list-style-type: none;
}
.menu__link {
  position: relative;
  font-size: 24px;
  line-height: 36px;
  font-weight: bold;
  margin-bottom: 15px;
  text-align: center;
}
.menu__link:hover {
  transform: scale(1.2);
}
.menu__link a {
  color: white;
  text-decoration: none;
  transition: all 0.6s;
}
.menu__link a:hover {
  color: #fbff06;
}
.menu__social {
  position: absolute;
  bottom: 50px;
  left: 50%;
  transform: translateX(-50%);
  width: 90px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.menu__social a:hover svg path {
  fill: #ffffff;
}
.menu__overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: #200505;
  opacity: 0.75;
}
.about {
  margin: 120px 0 0 0;
}
.about__descr {
  padding-top: 6px;
}
.about__photo img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.about__wrapper {
  width: 1040px;
  height: 560px;
  margin: 0 auto;
  display: grid;
  grid-template-columns: 400px 570px;
  grid-template-rows: repeat(2, 600px);
  gap: 70px;
}
.about__divider {
  width: 60px;
  height: 0px;
  border: 2px solid #FFA501;
  background: #FFA501;
}
.about__text {
  padding: 20px 0 40px 0;
  font-size: 14px;
  line-height: 21px;
}
.about__skills {
  display: grid;
  grid-template-columns: repeat(2, 260px);
  grid-template-rows: repeat(2, 116px);
  grid-auto-rows: minmax(116px);
  gap: 30px 30px;
}
.about__item {
  position: relative;
  padding-left: 50px;
}
.about__item span {
  font-size: 12px;
  padding-top: 6px;
}
.about__img {
  position: absolute;
  top: 10px;
  left: 15px;
}
.about__img:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -10px;
  left: -15px;
  width: 36px;
  height: 36px;
  border-radius: 100%;
  background: #FFA501;
}
.resume {
  padding-top: 120px;
}
.resume__title {
  text-align: center;
  position: relative;
}
.resume__title:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -25%;
  left: 0;
  width: 36px;
  height: 36px;
  border-radius: 100%;
  background: #FFA501;
}
.resume__title:before {
  top: -8px;
  left: 50%;
  transform: translateX(-50%);
}
.resume__subtitle {
  text-align: center;
}
.resume__divider {
  width: 60px;
  height: 0px;
  border: 2px solid #FFA501;
  background: #FFA501;
  margin: 5px auto;
}
.resume ul {
  list-style-type: none;
}
.resume ul li {
  position: relative;
}
.resume ul li:after {
  content: "";
  width: 1px;
  height: calc(100% + 16px);
  position: absolute;
  top: -8px;
  left: 0;
  background-color: black;
}
.resume__wrapper {
  padding: 60px 65px 0 65px;
  display: grid;
  grid-template-columns: repeat(2, 425px);
  column-gap: 135px;
}
.resume__col-title {
  position: relative;
  padding-left: 35px;
}
.resume__col-title:before {
  content: "";
  position: absolute;
  width: 15px;
  height: 15px;
  background: #FFA501;
  border-radius: 100%;
  left: -7px;
  top: 50%;
  transform: translateY(-50%);
}
.resume__item {
  position: relative;
  font-size: 12px;
  line-height: 18px;
}
.resume__item:before {
  content: "";
  height: 1px;
  width: 45px;
  background-color: black;
  position: absolute;
  top: 43px;
}
.resume__item-head {
  position: relative;
  padding: 25px 0 0 100px;
}
.resume__item-text {
  padding: 12px 0 0 43px;
}
.resume__item-icon {
  position: absolute;
  top: 25px;
  left: 45px;
}
.resume__item-icon:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -25%;
  left: 0;
  width: 36px;
  height: 36px;
  border-radius: 100%;
  background: #FFA501;
}
.resume__item-icon img {
  position: absolute;
  left: 16px;
  top: 14px;
}
.skills {
  margin-top: 120px;
}
.skills__tech {
  display: grid;
  grid-template-columns: repeat(4, 270px);
  grid-template-rows: repeat(2, 270px);
  gap: 13px;
  padding-top: 60px;
}
.skills__rating {
  margin-top: 120px;
}
.skills__tech-item {
  width: 270px;
  height: 270px;
  border: 1px solid black;
  padding: 30px 29px 25px 30px;
}
.skills__tech-item img {
  padding-bottom: 20px;
}
.skills__tech-item img.jquery {
  padding-top: 27px;
}
.skills__tech-item img.mongo {
  padding-top: 21px;
}
.skills__tech-item p {
  padding-top: 15px;
  font-size: 12px;
}
.skills__rating_wrapper {
  display: grid;
  grid-template-columns: repeat(3, 350px);
  grid-template-rows: repeat(2, 70px);
  grid-auto-rows: 70px;
  gap: 30px 45px;
}
.skills__rating-item {
  display: grid;
  grid-template: 21px 14px/200px 35px;
  justify-content: space-between;
  align-content: space-between;
}
.skills__ratings-line {
  width: 350px;
  height: 14px;
  background-color: #E8E4DE;
  border-radius: 4px;
  grid-column: 1/3;
}
.skills__ratings-line span {
  display: block;
  height: 100%;
  background-color: #FFA501;
  border-radius: 4px;
}
.skills__mixin {
  position: relative;
  z-index: 2;
  padding-left: 10px;
  top: -35%;
}
.skills__mixin:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: -35%;
  left: 0;
  width: 36px;
  height: 36px;
  border-radius: 100%;
  background: #FFA501;
}
.examples {
  margin-top: 120px;
}
.examples__wrapper {
  margin-top: 60px;
  display: grid;
  grid-template: repeat(2, 280px)/repeat(4, 280px);
  grid-auto-rows: 280px;
  gap: 5px;
}
.examples__wrapper .vertical {
  grid-row: 1/3;
  grid-column: 4/5;
}
.examples__wrapper .horizontal {
  grid-row: 2/3;
  grid-column: 2/4;
}
.examples__item {
  transition: all 0.6s;
}
.examples__item img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.price {
  margin-top: 120px;
}
.price__title {
  padding-bottom: 25px;
}
.price__wrapper {
  margin-top: 50px;
  padding: 0 50px;
  display: grid;
  grid-template: repeat(3, minmax(45px, auto))/repeat(2, 365px);
  grid-auto-rows: minmax(45px, auto);
  row-gap: 15px;
  justify-content: space-between;
}
.price__item {
  position: relative;
  display: grid;
  grid-template: repeat(2, minmax(21px, auto))/repeat(2, auto);
  row-gap: 5px;
  padding-left: 25px;
}
.price__item:before {
  content: "";
  width: 15px;
  height: 15px;
  background-color: #FFA501;
  border-radius: 100%;
  position: absolute;
  top: 5px;
}
.price__cost {
  justify-self: end;
  font-weight: bold;
}
.price__descr {
  grid-column: 1/3;
}
.contacts {
  padding: 100px 0 90px 0;
}
.contacts__wrapper {
  padding: 0px 35px 0 52px;
  display: grid;
  grid-template-columns: 400px 585px;
  justify-content: space-between;
}
.contacts__photo img {
  width: 100%;
  object-fit: cover;
}
.contacts__descr {
  padding-top: 6px;
}
.contacts__txt, .contacts__social {
  margin-top: 15px;
  font-weight: bold;
}
.contacts__link {
  margin-right: 25px;
  text-decoration: none;
}
.contacts__link img {
  max-width: 30px;
  max-height: 30px;
}
.contacts__link:hover {
  text-decoration: none;
}
.contacts__form {
  margin-top: 20px;
  display: grid;
  grid-template: 45px 190px 45px/repeat(2, 280px);
  justify-content: space-between;
  row-gap: 20px;
}
.contacts__triggers {
  grid-column: 1/3;
  display: grid;
  grid-template-columns: repeat(2, auto);
  column-gap: 35px;
}
.contacts__policy {
  font-family: popins;
  display: flex;
  align-items: center;
}
.contacts__policy input {
  margin-right: 10px;
}
.contacts__input {
  position: relative;
}
.contacts__input input {
  width: 100%;
  height: 100%;
  padding: 0 20px;
  font-size: 12px;
}
.contacts__txtarea {
  grid-column: 1/3;
  position: relative;
}
.contacts textarea {
  width: 100%;
  height: 100%;
  resize: none;
  padding: 20px;
  font-size: 12px;
}
label {
  display: block;
  top: -9px;
  left: 12px;
  margin-bottom: 0;
  position: absolute;
  height: 18px;
  padding: 0 8px;
  background-color: #fff;
  font-size: 12px;
}
.contacts__btn {
  border: none;
  font-size: 14px;
}
.about__divider {
  width: 60px;
  height: 0px;
  border: 2px solid #FFA501;
  background: #FFA501;
}
@media (max-width: 1400px) {
  .sidepanel {
    display: none;
  }
}
@media (max-width: 1200px) {
  .container {
    max-width: 960px;
  }

  .about__wrapper {
    grid-template-columns: 400px 520px;
    gap: 35px;
  }

  .about__skills {
    column-gap: 20px;
    grid-template: repeat(2, minmax(115px, auto))/repeat(2, 250px);
  }
}
@media (max-width: 992px) {
  .container {
    max-width: 720px;
  }

  .about {
    margin-top: 50px;
  }

  .about__wrapper {
    grid-template-columns: 300px 385px;
    gap: 35px;
  }

  .about__skills {
    row-gap: 15px;
    grid-template-columns: 380px;
    grid-auto-rows: minmax(115px, auto);
  }

  .about__text {
    padding-bottom: 15px;
  }
}
@media (max-width: 768px) {
  .container {
    max-width: 540px;
    margin: 0 10px;
  }

  .promo {
    padding-top: 100px;
  }

  .fz48 {
    font-size: 28px;
    line-height: 39px;
  }

  .promo__title {
    margin-top: 10px;
    width: 300px;
  }

  .promo__link-white {
    color: white;
  }

  .about__wrapper {
    width: 100vw;
    height: 100%;
    grid-template: 340px auto auto/1fr;
    gap: 35px;
  }

  .about__photo {
    display: none;
  }

  .about__skills {
    row-gap: 30px;
    grid-template-columns: repeat(2, 260px);
    grid-auto-rows: minmax(115px, auto);
  }

  .about__descr {
    width: 100%;
  }

  .resume__wrapper {
    padding: 20px 10px;
    grid-template-columns: 1fr;
  }

  .skills {
    margin-top: 30px;
  }

  .skills__tech {
    grid-template-columns: repeat(2, 270px);
  }

  .skills__rating {
    margin-top: 60px;
  }

  .skills__rating_wrapper {
    grid-template-columns: 1fr;
  }

  .skills__ratings-line {
    width: 100%;
    height: 14px;
    background-color: #E8E4DE;
    border-radius: 4px;
  }

  .examples {
    margin-top: 50px;
